{
  "name": "islyngten",
  "version": "1.1.0",
  "description": "Islyngten is a tiny JavaScript library supplying I18N translation support for Node.js and the browser.",
  "main": "islyngten.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "grunt build",
    "coverage": "nyc ./node_modules/.bin/jasmine",
    "lint": "grunt jshint",
    "test": "./node_modules/.bin/jasmine && npx karma start test/karma/karma.conf.js --single-run",
    "test-node": "./node_modules/.bin/jasmine",
    "test-browser": "npx karma start test/karma/karma.conf.js --single-run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/belexos/islyngten.git"
  },
  "keywords": [
    "i18n",
    "internationalization",
    "internationalisation",
    "interpolation",
    "translation",
    "globalization",
    "singular",
    "plural",
    "gettext"
  ],
  "author": "Br00ze <br00ze@belexos.com> (http://belexos.com)",
  "license": "MIT",
  "homepage": "https://github.com/belexos/islyngten#readme",
  "nyc": {
    "exclude": [
      "**/*Spec.js"
    ],
    "reporter": [
      "text",
      "html"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "grunt": "^1.3.0",
    "grunt-contrib-jshint": "^2.1.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-terser": "^1.0.0",
    "jasmine": "^3.6.1",
    "karma": "^6.3.16",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-firefox-launcher": "^1.3.0",
    "karma-jasmine": "^4.0.1",
    "nyc": "^15.1.0"
  }
}
